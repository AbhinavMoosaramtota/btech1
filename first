//PALINDROME
#include<stdio.h>
int main(void){
int x;
scanf("%d",&x);
int t=x;
int r=0,sum,rem;
while(x!=0){
rem=x%10;
r=(r*10)+rem;
x=x/10;
if (x==r)
printf("YES");
else
printf("NO");                                                                                                                                                                                        
}
}
